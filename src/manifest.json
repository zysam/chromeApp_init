{
    "name": "chromeApp_init",
    "description": "A chrome app init.",
    "manifest_version": 2,
    "version": "1.0.0",
    "minimum_chrome_version": "40",
    "app": {
        "background": {
            "scripts": [
                "main.js",
                "chromereload.js"
            ]
        }
    },
    "icons": {
        "16": "images/icon.png",
        "64": "images/icon.png",
        "128": "images/icon.png"
    },
    "permissions": [
        "storage",
        "<all_urls>"
    ]
}